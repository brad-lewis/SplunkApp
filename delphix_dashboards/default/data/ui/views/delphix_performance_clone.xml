  <row>
    <panel>
      <title>iSCSI Operations</title>
      <chart>
        <search>
          <query>| mstats avg(_value) as io_count where `delphix_metrics_index` AND metric_name="system.iscsi.ops.count" host="$host$" span=1s by data.op, data.avgLatency, data.throughput | eval operation='data.op' | eval latency = ('data.avgLatency' / 1000000) | eval thruput= round('data.throughput' / 1024 , 2) | timechart span=10m avg(io_count) as "I/O count", avg(latency) as "AvgLatency (ms)", avg(thruput) as "Throughput (KB)" by operation</query>
          <earliest>$timeRange.earliest$</earliest>
          <latest>$timeRange.latest$</latest>
          <sampleRatio>1</sampleRatio>
          <refresh>10m</refresh>
          <refreshType>delay</refreshType>
        </search>
        <option name="charting.axisTitleX.visibility">collapsed</option>
        <option name="charting.axisTitleY.visibility">collapsed</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.axisY.abbreviation">auto</option>
        <option name="charting.chart">area</option>
        <option name="charting.chart.nullValueMode">connect</option>
