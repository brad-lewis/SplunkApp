        <option name="trellis.scales.shared">0</option>
        <option name="trellis.size">large</option>
      </chart>
    </panel>
    <panel>
      <title>Dataset Performance</title>
      <chart>
        <search>
          <query>| mstats avg(_value) as Val where `delphix_metrics_index` AND host="$host$" metric_name="system.dataset.total.bytes" span=1s by data.dataset, data.nread, data.nwritten, data.type
| eval dataset='data.dataset' 
